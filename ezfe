	end)


	Section:NewToggle("Crash auto", "Others", function(state)
	    if state then
	_G.on = true

	while _G.on == true do
	    wait()

	wait(0.2)
	local args = {
	        [1] = "Jet"
	    }
	    game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
	        local args = {
	        [1] = "Jet"
	    }
	    game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))

	wait(0.2)

	    end
	    else
	_G.on = false
	        
	    end
	end)

